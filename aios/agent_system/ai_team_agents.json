  "communication_rules": [
    {
      "rule": "调用格式",
      "description": "通过前缀调用Agent",
      "example": "@product-lead 请分析用户反馈，制定优化方案"
    },
    {
      "rule": "任务传递",
      "description": "Agent完成任务后自动传递给下一个Agent",
      "example": "product-lead完成需求分析 → 自动通知fullstack-dev开始开发"
    },
    {
      "rule": "汇报机制",
      "description": "每个Agent完成任务后向上级汇报",
      "example": "fullstack-dev完成开发 → 汇报给product-lead → 汇报给ceo"
    },
    {
      "rule": "协作模式",
      "description": "Agent之间可以互相请求协助",
      "example": "fullstack-dev遇到技术问题 → 请求backend-specialist协助"
